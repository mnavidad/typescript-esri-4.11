{
    "compilerOptions": {
      "module": "amd",
      "noImplicitAny": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "jsx": "react",
      "jsxFactory": "tsx",
     "lib": ["es2015","es2015.collection","es2015.promise"],
      "target": "es6",
      "experimentalDecorators": true,
      "preserveConstEnums": true,
      "suppressImplicitAnyIndexErrors": true
    },
    "include": [
      "./app/*"
    ],
    "exclude": [
      "node_modules"
    ]
  }